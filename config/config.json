{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "employee-report-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/employeeReport/EmployeeReportWebPart.js",
          "manifest": "./src/webparts/employeeReport/EmployeeReportWebPart.manifest.json"
        }
      ]
    },
    "employee-document-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/employeeDocument/EmployeeDocumentWebPart.js",
          "manifest": "./src/webparts/employeeDocument/EmployeeDocumentWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "EmployeeReportWebPartStrings": "lib/webparts/employeeReport/loc/{locale}.js",
    "EmployeeDocumentWebPartStrings": "lib/webparts/employeeDocument/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}